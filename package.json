{
	"name": "iterator-result",
	"version": "0.0.2",
	"description": "A basic iterator result.",
	"homepage": "https://github.com/vanruesc/iterator-result",
	"main": "build/iterator-result.js",
	"module": "src/index.js",
	"sideEffects": false,
	"license": "Zlib",

	"keywords": [
		"iterator",
		"result",
		"protocol"
	],

	"author": {
		"name": "Raoul van RÃ¼schen",
		"email": "vanruesc@outlook.de"
	},

	"repository": {
		"type": "git",
		"url": "https://github.com/vanruesc/iterator-result.git"
	},

	"bugs": {
		"url": "https://github.com/vanruesc/iterator-result/issues"
	},

	"files": [
		"src",
		"build"
	],

	"scripts": {
		"lint": "npm run lint:lib && npm run lint:test",
		"lint:config": "eslint *.js",
		"lint:lib": "eslint src",
		"lint:test": "eslint test",
		"build": "rollup -c",
		"build:production": "cross-env NODE_ENV=production rollup -c",
		"doc": "esdoc",
		"test": "ava",
		"prepack": "npm run lint && npm run build:production"
	},

	"engines": {
		"node": ">=4.0.0",
		"npm": ">=4.0.0"
	},

	"ava": {
		"failFast": true,
		"files": ["test/**/*.js"]
	},

	"eslintConfig": {
		"extends": "delta"
	},

	"dependencies": {},

	"devDependencies": {
		"ava": "0.x.x",
		"babel-core": "6.x.x",
		"babel-plugin-external-helpers": "6.x.x",
		"babel-preset-env": "1.x.x",
		"cross-env": "5.x.x",
		"esdoc": "1.x.x",
		"esdoc-standard-plugin": "1.x.x",
		"eslint": "5.x.x",
		"eslint-config-delta": "0.x.x",
		"rollup": "0.x.x",
		"rollup-plugin-babel": "3.x.x",
		"rollup-plugin-babel-minify": "5.x.x",
